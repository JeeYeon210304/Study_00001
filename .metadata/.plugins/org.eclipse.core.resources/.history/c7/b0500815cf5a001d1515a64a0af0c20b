package test;

class Data1 { int x; }

public class WhatIsTheStatic {

	
	int a = 1;
	
	static int b = 1;
	
	public static void main(String[] args) {
		WhatIsTheStatic what = new WhatIsTheStatic();
		what.rtn();
		
		testt(2);
		System.out.println(testt(2).x);
	}
	
	public int rtn() {
		System.out.println(a);
		return 0;
	}
	
	public static Data1 testt (int a) {
		Data1 d = new Data1();
		d.x = a;
		return d;
	}
	
}
